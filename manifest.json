{
  "name": "Getting Started Example",
  "version": "1.0",
  "description": "Build an Extension!",
  "manifest_version": 2,
  "content_scripts":[
    {
      "matches": ["<all_urls>"],
      "js": ["rgb.js","content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "hello_extensions.png",
    "default_popup": "popup.html",
    "default_title": "My popup!"
  }
}